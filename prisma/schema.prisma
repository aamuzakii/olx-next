// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model House {
  id           Int       @id @default(autoincrement())
  url          String    @unique
  feature      String?
  title        String?
  price        Int?
  score        Int?
  prefecture   String?
  published    DateTime?
  publishedStr String?
  imageUrl     String?
  comment      String?
  color        String?
  standard     String?
  deleted      Boolean?  @default(false)
}

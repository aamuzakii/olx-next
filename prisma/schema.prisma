// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGODB_URI") // Replace with your MongoDB connection string
}

model House {
  id             String    @id @default(auto()) @map("_id") @db.ObjectId
  url            String    @unique
  feature        String?
  title          String?
  price          Int?
  score          Int?      @default(0)
  prefecture     String?
  published      DateTime?
  publishedStr   String?
  imageUrl       String?
  comment        String?
  warteg         String?
  schoolDistance String?
  workDistance   String?
  noNoise        String?
  flood          String?
  gateOrCanopy   String?
  dividable      String?
  color          String?
  standard       String?
  deleted        Boolean?  @default(false)
}

model Job {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  url         String   @unique
  title       String   @default("")
  stack       String?
  country     String?
  fee         String?
  candidates  String?
  description String?
  date        String?
  deleted     Boolean? @default(false)
}
